{
    "manifest_version": 2,
    "name": "M4rked",
    "version": "1.0",
  
    "description": "Allows users to mark domains, and will show if the domain isnt marked.",
  
    "icons": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    },

    "permissions": [
      "activeTab",
      "tabs",
      "storage"
    ],

    "browser_action": {
      "default_icon": "icons/icon-32.png",
      "default_title": "DomProt"
    },

    "background": {
      "scripts": ["bg.js"]
    },

    "author": "Booklordofthedings"
  
  
  }
  